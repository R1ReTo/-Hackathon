{% extends 'base.html.twig' %}

{% block title %}Hello SerieController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">

		<table data-toggle="table" data-url="http://localhost:8000/getHackathon" data-pagination="true" data-search="true">
			<thead>
				<tr>
					<th data-sortable="true" data-field="idhackathon">Item ID</th>
					<th data-field="datedebut">Item Name</th>
					<th data-field="heuredebut">Item Price</th>
				</tr>
			</thead>
		</table>
	</div>
	{% endblock %}
