{% extends 'base.html.twig' %}

{% block title %}Hello SerieController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

   {% for uneHackathon in lesHackathon %}
        
        
        <div class="card" style="width: 18rem;">
            <img src={{uneSerie.image}} class="card-img-top">
                <div class="card-body">
                <h5 class="card-title">{{uneHackathon.lieu}}</h5>
                <p class="card-text">{{uneHackathon.rue}}</p>
                <p class="card-text">{{uneHackathon.ville}}</p>
                <p class="card-text">{{uneHackathon.cp}}</p>
            </div>
        </div>
        <br>

    {% endfor %} 
    
</div>
{% endblock %}